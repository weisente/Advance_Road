<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ProgressView">
        <attr name="maxProgressColor" format="color" />
        <attr name="currentProgressColor" format="color" />
        <attr name="progressborderWidth" format="dimension" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="progressTextColor" format="color" />
    </declare-styleable>

    <!--<declare-styleable name="maxProgressColor">asd</declare-styleable>-->

    <declare-styleable name="Follow">
        <attr name="target" format="reference"/>
    </declare-styleable>

    <declare-styleable name="Mytextview">
        <attr name="mytext" format="string"/>
        <attr name="mytextsize" format="dimension"/>
        <attr name="mycolor" format="color"/>
    </declare-styleable>


    <declare-styleable name="ColorTrackTextView">
        <attr name="originColor" format="color"/>
        <attr name="changeColor" format="color"/>
    </declare-styleable>



    <declare-styleable name="QQStepView">
        <attr name="outerColor" format="color" />
        <attr name="innerColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="stepTextSize" format="dimension" />
        <attr name="stepTextColor" format="color" />
    </declare-styleable>

    <declare-styleable name="RatingBar">
        <attr name="starNormal" format="reference"/>
        <attr name="starFocus" format="reference"/>
        <attr name="gradeNumber" format="integer"/>
        <attr name="interval" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SlidingMenu">
        <attr name="menuRightMargin" format="dimension"/>
    </declare-styleable>

</resources>